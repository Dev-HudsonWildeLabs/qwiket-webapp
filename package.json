{
  "name": "qwiket-webapp",
  "version": "0.0.2",
  "description": "Isomorphic React App for Qwiket.com",
  "repository": {
    "type": "git",
    "url": "git://github.com/nicholasdickey/qwiket-webapp.git"
  },
  "homepage": "https://github.com/RickWong/react-isomorphic-starterkit",
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "starter",
    "boilerplate",
    "template",
    "webpack",
    "hapi",
    "transmit"
  ],
  "main": "babel.server.js",
  "scripts": {
    "start": "node ./babel.server.js",
    "build": "node ./node_modules/webpack/bin/webpack.js --verbose --colors --display-error-details --config webpack.client.js",
    "watch-client": "node ./node_modules/webpack/bin/webpack.js --verbose --colors --display-error-details --config webpack.client-watch.js && node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.client-watch.js",
    "watch": "node ./node_modules/concurrently/src/main.js --kill-others \"npm run watch-client\" \"npm run start\""
  },
  "dependencies": {
    "babel": "5.8.23",
    "babel-core": "^5.8.25",
    "babel-plugin-typecheck": "1.3.0",
    "compression": "^1.6.0",
    "domino": "^1.0.19",
    "es6-mapify": "^1.0.0",
    "express": "^4.13.3",
    "h2o2": "4.0.1",
    "hapi": "9.3.1",
    "history": "1.12.4",
    "http-proxy": "^1.11.2",
    "immutable": "^3.7.5",
    "inert": "3.1.0",
    "isomorphic-fetch": "2.1.1",
    "jquery": "^2.1.4",
    "lodash": "^3.10.1",
    "moment": "^2.8.4",
    "node-jsx": "^0.12.4",
    "object-assign": "^2.0.0",
    "path": "^0.12.7",
    "piping": "0.3.0",
    "pretty-error": "^1.2.0",
    "radium": "^0.14.1",
    "react": "0.14.0",
    "react-addons-css-transition-group": "^0.14.0",
    "react-addons-update": "^0.14.0",
    "react-burger-menu": "^1.3.0",
    "react-disqus-thread": "^0.2.2",
    "react-dom": "0.14.0",
    "react-inline-css": "2.0.0",
    "react-radio-group": "^2.1.1",
    "react-redux": "^4.0.0",
    "react-router": "1.0.0-rc3",
    "react-transmit": "3.0.7",
    "redux": "^3.0.2",
    "redux-thunk": "^1.0.0",
    "serve-favicon": "^2.3.0",
    "serve-static": "^1.10.0",
    "sorted-array": "^2.0.1",
    "webpack-isomorphic-tools": "^0.9.1",
    "wreck": "^6.3.0",
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "babel-core": "5.8.25",
    "babel-loader": "5.3.2",
    "babel-runtime": "5.8.25",
    "concurrently": "0.1.1",
    "imports-loader": "^0.6.4",
    "json-loader": "0.5.3",
    "react-hot-loader": "1.3.0",
    "redux-devtools": "^2.1.5",
    "webpack": "1.12.2",
    "webpack-dev-server": "1.12.0"
  },
  "engines": {
    "node": ">=0.10.32"
  }
}
